{
    "name": "lojametastore/melhorenvio-sdk-php",
    "description": "Este pacote é responsável por realizar o cálculo de cotações.",
    "keywords": [
        "melhorenvio",
        "shipment-sdk-php",
        "melhorenvio-sdk-php"
    ],
    "homepage": "https://github.com/orbitalcodes/shipment-sdk-php",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Melhor Envio",
            "email": "tecnologia@melhorenvio.com",
            "role": "Developer"
        },
        {
            "name": "Kallef Alexandre",
            "email": "kallef.alexandre@lojametastore.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0 || ^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5"
    },
    "autoload": {
        "psr-4": {
            "MelhorEnvio\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MelhorEnvio\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
